  "dependencies": {
    "bignumber.js": "9.0.0",
    "ethers": "5.0.0-beta.187",
    "lit-html": "1.2.1",
    "reduxular": "0.0.6"
  },
  "devDependencies": {
    "@types/node": "14.0.4",
    "node-fetch": "2.6.0",
